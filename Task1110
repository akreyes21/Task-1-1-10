class Book {
    String title;
    String author;
    int yearPublished;
    double price;
    
    public Book(String title, String author, int yearPublished, double price) {
        this.title = title;
        this.author = author;
        this.yearPublished = yearPublished;
        this.price = price;
    }
}

public class TestBookConstructor {
    public static void main(String[] args) {
        // Instantiate three Book objects with different values using the constructor
        Book book1 = new Book("Basic Mathematics", "Isaac Newton", 1965, 49.99);
        Book book2 = new Book("Literature", "Lualhati Bautista", 2015, 30.99);
        Book book3 = new Book("Rizal", "Gregorio Zaide", 2000, 20.99);
        
        // Print out the details of each book
        System.out.println("Book 1:");
        System.out.println("Title: " + book1.title);
        System.out.println("Author: " + book1.author);
        System.out.println("Year Published: " + book1.yearPublished);
        System.out.println("Price: $" + book1.price);
        System.out.println();
        
        System.out.println("Book 2:");
        System.out.println("Title: " + book2.title);
        System.out.println("Author: " + book2.author);
        System.out.println("Year Published: " + book2.yearPublished);
        System.out.println("Price: $" + book2.price);
        System.out.println();
        
        System.out.println("Book 3:");
        System.out.println("Title: " + book3.title);
        System.out.println("Author: " + book3.author);
        System.out.println("Year Published: " + book3.yearPublished);
        System.out.println("Price: $" + book3.price);
    }
}
